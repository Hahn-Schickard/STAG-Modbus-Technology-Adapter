{
    "log_to_standard_output": true,
    "logfile_name": "modbus_example.log",
    "logfile_path": "logs",
    "logging_level": "debug",
    "item_queue_size": 8192,
    "thread_count": 2,
    "maximum_file_count": 25,
    "maximum_file_size_MB": 100,
    "flush_period": 1,
    "message_pattern": "[%Y-%m-%d-%H:%M:%S:%F %z][%n]%^[%l]: %v%$"
}